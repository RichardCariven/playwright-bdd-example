{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "check-types": "tsc --noEmit",
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --log-level=warn",
    "lint": "eslint . --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@neshca/cache-handler": "1.3.2",
    "@next/bundle-analyzer": "14.2.4",
    "@next/third-parties": "14.2.5",
    "@rayo/fetch-client": "*",
    "@rayo/localisation": "*",
    "@rayo/ui": "*",
    "@reach/menu-button": "0.18.0",
    "@sentry/nextjs": "8.15.0",
    "chalk": "4.1.2",
    "dotenv": "16.4.5",
    "dotenv-webpack": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "nanoid": "5.0.7",
    "negotiator": "0.6.3",
    "next": "14.2.5",
    "pusher-js": "8.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "redis": "4.6.14",
    "swr": "2.2.5"
  },
  "devDependencies": {
    "@happy-dom/jest-environment": "14.12.3",
    "@rayo/eslint": "*",
    "@rayo/msw-handlers": "*",
    "@rayo/typescript-config": "*",
    "@storybook/jest": "0.2.3",
    "@storybook/react": "8.0.4",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@types/dotenv-webpack": "7.0.7",
    "@types/jsonwebtoken": "9.0.6",
    "@types/negotiator": "0.6.3",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.17",
    "dotenv-webpack": "*",
    "eslint": "8.56.0",
    "gzip-size": "6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "mkdirp": "3.0.1",
    "msw": "2.1.5",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.5.3",
    "undici": "6.19.2",
    "whatwg-fetch": "3.6.20"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "nextBundleAnalysis": {
    "budget": 204800,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
}
